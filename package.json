{
  "name": "@baic/code-editor",
  "version": "1.0.4",
  "description": "各种代码编辑器",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/303394539/code-editor.git"
  },
  "license": "MIT",
  "main": "lib/index.js",
  "module": "es/index.js",
  "typings": "lib/index.d.ts",
  "files": [
    "compiled",
    "lib",
    "es"
  ],
  "scripts": {
    "build": "father build",
    "lint": "eslint ./src --ext .ts,.tsx --fix",
    "prebundle": "father prebundle",
